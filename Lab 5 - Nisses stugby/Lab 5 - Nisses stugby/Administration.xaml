<Page x:Class="Lab_5___Nisses_stugby.Administration"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Lab_5___Nisses_stugby"
      mc:Ignorable="d" 
      d:DesignHeight="540" d:DesignWidth="860"
      Title="Administration">

    <Grid>
        <DataGrid x:Name="PeopleDatagrid" HorizontalAlignment="Left" Height="353" Margin="10,131,0,0" VerticalAlignment="Top" Width="624" AutoGeneratedColumns="PeopleDatagrid_AutoGeneratedColumns" RowEditEnding="PeopleDatagrid_RowEditEnding" PreviewKeyDown="PeopleDatagrid_PreviewKeyDown"/>
        <Label x:Name="PeopleLabel" Content="Registrerade personer (tryck på delete på tangentbordet för att ta bort någon)" HorizontalAlignment="Left" Margin="10,100,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Home" Content="Hem" HorizontalAlignment="Left" Margin="70,30,0,0" VerticalAlignment="Top" Width="75" Click="BtnHome_click"/>
        <Button x:Name="Registration" Content="Registrera" HorizontalAlignment="Left" Margin="199,30,0,0" VerticalAlignment="Top" Width="75" Click="Registration_Click"/>
        <Button x:Name="Search" Content="Sök stuga" HorizontalAlignment="Left" Margin="323,30,0,0" VerticalAlignment="Top" Width="75" Click="Search_Click"/>
        <Button x:Name="Contact" Content="Kontaktinfo" HorizontalAlignment="Left" Margin="447,30,0,0" VerticalAlignment="Top" Width="75" Click="Contact_Click"/>
        <Button x:Name="Weather" Content="Väder" HorizontalAlignment="Left" Margin="571,30,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="Admin" Content="Admin" HorizontalAlignment="Left" Margin="695,30,0,0" VerticalAlignment="Top" Width="75" Click="Admin_Click"/>
    </Grid>
</Page>
